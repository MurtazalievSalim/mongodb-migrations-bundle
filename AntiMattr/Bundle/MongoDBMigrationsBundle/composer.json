{
    "name": "salim/mongodb-migrations-bundle",
    "type": "symfony-bundle",
    "description": "Symfony MongoDBMigrationsBundle",
    "keywords": ["database", "doctrine", "migration", "mongodb", "symfony"],
    "homepage": "https://github.com/MurtazalievSalim/mongodb-migrations-bundlev",
    "license": "MIT",
    "authors": [
        {"name": "Matthew Fitzgerald", "email": "matthewfitz@gmail.com"}
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.1",        
        "salim/mongodb-migrations": "~1.0@stable",
        "doctrine/mongodb-odm": "1.*"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": { "AntiMattr\\Bundle\\MongoDBMigrationsBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "target-dir" : "AntiMattr/Bundle/MongoDBMigrationsBundle"
}
